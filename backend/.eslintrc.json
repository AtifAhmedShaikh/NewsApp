{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
            "indent": [
                "error"
           ],
            "global-require": [
                "error"
            ],
            "handle-callback-err": [
                "error"
            ]
    }
}
